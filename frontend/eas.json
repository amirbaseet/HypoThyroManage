{
  "cli": {
    "version": ">= 15.0.15",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "${API_URL}",
        "SOCKET_URL": "${SOCKET_URL}"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "${API_URL}",
        "SOCKET_URL": "${SOCKET_URL}"
      }
    },
    "production": {
      "autoIncrement": true,
      "credentialsSource": "remote",
      "env": {
        "API_URL": "${API_URL}",
        "SOCKET_URL": "${SOCKET_URL}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
